{
  "name": "qrany-web",
  "version": "0.0.1",
  "description": "ant中台页面",
  "workspaces": ["package", "layout", "login", "welcome"],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
